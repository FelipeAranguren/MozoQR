{
    "kind": "collectionType",
    "collectionName": "mesa_sesions",
    "info": {
        "singularName": "mesa-sesion",
        "pluralName": "mesa-sesions",
        "displayName": "MesaSesion"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "code": {
            "type": "uid",
            "required": true
        },
        "session_status": {
            "type": "enumeration",
            "default": "open",
            "enum": [
                "open",
                "closed",
                "paid"
            ]
        },
        "openedAt": {
            "type": "datetime"
        },
        "closedAt": {
            "type": "datetime"
        },
        "total": {
            "type": "integer",
            "default": 0
        },
        "paidTotal": {
            "type": "integer",
            "default": 0
        },
        "notes": {
            "type": "string"
        },
        "mesa": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::mesa.mesa",
            "inversedBy": "mesa_sesions"
        },
        "restaurante": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::restaurante.restaurante",
            "inversedBy": "mesa_sesions"
        },
        "pedidos": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::pedido.pedido",
            "mappedBy": "mesa_sesion"
        }
    }
}
