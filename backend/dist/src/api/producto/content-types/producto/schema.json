{
    "kind": "collectionType",
    "collectionName": "productos",
    "info": {
        "singularName": "producto",
        "pluralName": "productos",
        "displayName": "Producto"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "price": {
            "type": "decimal",
            "required": true
        },
        "description": {
            "type": "blocks"
        },
        "restaurante": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::restaurante.restaurante",
            "inversedBy": "productos"
        },
        "image": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "categoria": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::categoria.categoria",
            "inversedBy": "productos"
        },
        "item_pedidos": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::item-pedido.item-pedido",
            "mappedBy": "product"
        },
        "slug": {
            "type": "uid"
        },
        "available": {
            "type": "boolean",
            "default": true
        },
        "sku": {
            "type": "string"
        },
        "extras": {
            "type": "json"
        }
    }
}
