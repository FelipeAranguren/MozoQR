{
    "kind": "collectionType",
    "collectionName": "pedidos",
    "info": {
        "singularName": "pedido",
        "pluralName": "pedidos",
        "displayName": "Pedido"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "table": {
            "type": "integer"
        },
        "order_status": {
            "type": "enumeration",
            "default": "pending",
            "enum": [
                "pending",
                "preparing",
                "served",
                "paid"
            ]
        },
        "items": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::item-pedido.item-pedido",
            "mappedBy": "order"
        },
        "total": {
            "type": "decimal"
        },
        "customerNotes": {
            "type": "text"
        },
        "restaurante": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::restaurante.restaurante",
            "inversedBy": "pedidos"
        },
        "users_permissions_user": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "pedidos"
        },
        "tableSessionId": {
            "type": "string"
        }
    }
}
