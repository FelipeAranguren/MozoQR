{
    "kind": "collectionType",
    "collectionName": "restaurantes",
    "info": {
        "singularName": "restaurante",
        "pluralName": "restaurantes",
        "displayName": "Restaurante"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "productos": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::producto.producto",
            "mappedBy": "restaurante"
        },
        "categorias": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::categoria.categoria",
            "mappedBy": "restaurante"
        },
        "logo": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "slug": {
            "type": "uid",
            "targetField": "name"
        },
        "primaryColoraccentColor": {
            "type": "string"
        },
        "tipografia": {
            "type": "string"
        },
        "pedidos": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::pedido.pedido",
            "mappedBy": "restaurante"
        },
        "mp_access_token": {
            "type": "text",
            "private": true
        },
        "mp_public_key": {
            "type": "string",
            "private": true
        },
        "cbu": {
            "type": "text"
        },
        "cuenta_bancaria": {
            "type": "string"
        },
        "mesas": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::mesa.mesa",
            "mappedBy": "restaurante"
        },
        "mesa_sesions": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::mesa-sesion.mesa-sesion",
            "mappedBy": "restaurante"
        },
        "restaurant_members": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::restaurant-member.restaurant-member",
            "mappedBy": "restaurante"
        },
        "Suscripcion": {
            "type": "enumeration",
            "required": true,
            "default": "basic",
            "enum": [
                "basic",
                "pro",
                "ultra"
            ]
        }
    }
}
